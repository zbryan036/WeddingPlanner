@model Wedding
<div class="Container">
    <div class="row" style="margin-top:50px">
        <div class="col-3"></div>
        <div class="col-6">
            <h2 class="display-4">Plan Your Wedding</h2>
        </div>
        <div class="col-3"></div>
    </div>
    <div class="row">
        <div class="col-7"></div>
        <div class="col-1">
            <a href="/">Dashboard </a>
        </div>
        <div class="col-2">
            <a href="/logout"> Log Out</a>
        </div>
        <div class="col-2"></div>
    </div>
    <div class="row">
        <div class="col-3"></div>
        <div class="col-6">
            <form asp-action="CreateWedding" method="post">
                <div class="form-group">
                    <span asp-validation-for="Wedder1"></span>
                    <label asp-for="Wedder1"></label>
                    <input asp-for="Wedder1" class="form-control">
                </div>
                <div class="form-group">
                    <span asp-validation-for="Wedder2"></span>
                    <label asp-for="Wedder2"></label>
                    <input asp-for="Wedder2" class="form-control">
                </div>
                <div class="form-group">
                    <span asp-validation-for="Date"></span>
                    <label asp-for="Date"></label>
                    <input asp-for="Date" class="form-control">
                </div>
                <div class="form-group">
                    <span asp-validation-for="Address"></span>
                    <label asp-for="Address"></label>
                    <input asp-for="Address" class="form-control">
                </div>
                <div class="form-group">
                    <button type="submit" class="btn btn-primary">Create</button>
                </div>
            </form>
        </div>
        <div class="col-3"></div>
    </div>
</div>