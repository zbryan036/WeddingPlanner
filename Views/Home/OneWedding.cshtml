<div class="Container">
    <div class="row" style="margin-top:50px">
        <div class="col-3"></div>
        <div class="col-6">
            <h2 class="display-5">@ViewBag.Wedding.Wedder1 & @ViewBag.Wedding.Wedder2's Wedding</h2>
        </div>
        <div class="col-1">
            <a href="/">Dashboard </a>
        </div>
        <div class="col-2">
            <a href="/logout"> Log Out</a>
        </div>
        <div class="row">
            <div class="col-3"></div>
            <div class="col-6">
                <p>Date: @ViewBag.Wedding.Date.ToString("dd/M/yyyy")</p>
                <p>Guests: </p>
                <ul>
                    @foreach(RSVP r in ViewBag.Wedding.RSVPs) {
                        <li>@r.U.FirstName @r.U.LastName</li>
                    }
                </ul>
            </div>
            <div class="col-3"></div>
        </div>
    </div>